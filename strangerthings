use_bpm 140
use_synth :saw
live_loop :strangerThings do
  play :c2
  sleep 0.5
  play :e2
  sleep 0.5
  play :g2
  sleep 0.5
  play :b2
  sleep 0.5
  play :c3
  sleep 0.5
  play :b2
  sleep 0.5
  play :g2
  sleep 0.5
  play :e2
  sleep 0.5
  with_fx :flanger,  feedback: 0.6 do
    sample :guit_em9
  end
end
live_loop :drums do
  sample :bass_thick_c
  sleep 0.5
end
